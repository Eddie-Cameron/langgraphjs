# Database connection (use container name inside network, localhost for dev)
SUPABASE_DB_URL=postgresql://postgres:postgres@supabase-langgraph:5432/langgraph

# Security keys (development environment)
# NOTE: Replace these placeholder values with actual development keys
# Do not commit actual keys to version control
SUPABASE_JWT_SECRET=dev_jwt_secret_placeholder
SUPABASE_SERVICE_ROLE_KEY=dev_service_key_placeholder

# LangGraph API keys
# NOTE: Replace these placeholder values with actual development keys
OPENAI_API_KEY=dev_openai_key_placeholder
ANTHROPIC_API_KEY=dev_anthropic_key_placeholder
GOOGLE_API_KEY=dev_google_key_placeholder

# Redis
REDIS_URL=redis://langgraph-redis:6379